<form action="login" method="post">

			<div class="pageAlign">

				<label for="mailId" style="color: #FDFEFE;"><strong>EMAIL
						ID </strong></label><br> <input type="email" id="mailId" name="email" required
					placeholder="example@gmail.com"
					pattern="[a-z][a-z0-9]+[@][a-z]+[.][a-z]+"
					 autofocus> &nbsp;&nbsp;<br>
				<br> <label for="password" style="color: #FDFEFE;"><strong>PASSWORD</strong>
				</label> <br> <input type="password" id="password" name="pass"
					style="border-radius: 10px; left: 50%; right: 50%;" required
					placeholder="*********" pattern="[A-Za-z0-9@#.!&]{8,16}">
				&nbsp;
				<div class="checkBox">
					<input type="checkbox" onclick="myFunction()"> <label
						class="showPassword">Show Password</label>
				</div>
				<button style="border-radius: 8px;" type="submit" class="submitBtn">submit</button>
				<br> <br>
			</div>
		</form>






